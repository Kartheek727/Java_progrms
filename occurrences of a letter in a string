import java.util.*;
class prob{
    public static void main(String[] args){
        Scanner obj=new Scanner(System.in);
        String st=obj.nextLine();
        char tr=obj.next().charAt(0);
        int count=0;
        for(int i=0;i<st.length();i++){
            if(st.charAt(i)==tr){
                count++;
            }
        }
        System.out.println(count);
    }
}
